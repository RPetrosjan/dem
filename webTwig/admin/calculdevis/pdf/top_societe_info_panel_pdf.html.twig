<table class="topsocieteinfo">
    <tr>
        <td>
            <div class="namesociete colred">{{ societe_info.namesociete }}</div>
            <span>{{ societe_info.adresse }}</span>
            <div>
                <span>{{ societe_info.cpville.cp }} {{ societe_info.cpville.ville }}</span>
            </div>

                {% for telsoc in societe_info.tel %}
                    <span class="colred">{{ telsoc.tel }}</span> /
                {% endfor %}

        </td>
        <td style="text-align: right">
            <strong>N SIRET {{ societe_info.siret }}</strong><br/>
            <span>{{ societe_info.email }}</span><br/>
            <span>{{ societe_info.website }}</span>
        </td>
    </tr>
</table>
